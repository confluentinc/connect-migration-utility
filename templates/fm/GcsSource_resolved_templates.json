{
  "templates": [
    {
      "template_id": "GcsSource",
      "connector_type": "SOURCE",
      "connector.class": "io.confluent.connect.gcs.GcsSourceConnector",
      "config_defs": [
        {
          "name": "topic.regex.list",
          "type": "LIST",
          "required": true,
          "importance": "HIGH",
          "order_in_group": 1,
          "group": "Which topic(s) do you want to send data to?",
          "display_name": "Topic Name Regex Patterns",
          "documentation": "A list of topics along with a regex expression of the files which are to be sent to that topic.  For example: \"my-topic:.*\" will send all files to \"my-topic\", while a list containing only the expression \"special-topic:.*\\.json\" will send only files starting with \".json\" to \"special-topic\", and all other files not matching any patterns will be ignored and not sourced. Files that match multiple mappings will be sent to the first topic in the list that maps the file. The ``topic.regex.list`` property matches the full GCS path (for example, ``folder/file.txt``), not just the filename."
        },
        {
          "name": "gcs.credentials.json",
          "type": "PASSWORD",
          "required": true,
          "importance": "HIGH",
          "group": "GCP credentials",
          "order_in_group": 1,
          "display_name": "GCP credentials file",
          "documentation": "GCP service account JSON file with read permissions for Google Cloud Storage.",
          "sanitizers": [
            {
              "name": "gcp.credentials.sanitize"
            }
          ]
        },
        {
          "name": "gcs.bucket.name",
          "type": "STRING",
          "required": true,
          "importance": "HIGH",
          "group": "Google Cloud Storage details",
          "order_in_group": 1,
          "display_name": "GCS Bucket name.",
          "documentation": "The name of the GCS bucket."
        },
        {
          "name": "gcs.part.retries",
          "importance": "MEDIUM",
          "type": "INT",
          "default_value": "3",
          "group": "Google Cloud Storage details",
          "order_in_group": 2,
          "display_name": "GCS Part Upload Retries",
          "documentation": "Number of upload retries of a single GCS part. Zero means no retries"
        },
        {
          "name": "gcs.retry.backoff.ms",
          "importance": "MEDIUM",
          "type": "INT",
          "default_value": "200",
          "group": "Google Cloud Storage details",
          "order_in_group": 3,
          "display_name": "Retry Backoff (ms)",
          "documentation": "How long to wait in milliseconds before attempting the first retry of a failed GCS request. Upon a failure, this connector may wait up to twice as long as the previous wait, up to the maximum number of retries. This avoids retrying in a tight loop under failure scenarios."
        },
        {
          "name": "topics.dir",
          "type": "STRING",
          "default_value": "topics",
          "importance": "HIGH",
          "group": "Storage",
          "order_in_group": 1,
          "display_name": "Topics directory",
          "documentation": "Top-level directory (in the GCS bucket) where data to be ingested is stored."
        },
        {
          "name": "directory.delim",
          "type": "STRING",
          "default_value": "/",
          "importance": "MEDIUM",
          "group": "Storage",
          "order_in_group": 2,
          "display_name": "Directory Delimiter Character",
          "documentation": "Directory delimiter pattern."
        },
        {
          "name": "behavior.on.error",
          "type": "STRING",
          "default_value": "FAIL",
          "importance": "HIGH",
          "group": "Storage",
          "order_in_group": 3,
          "display_name": "Behavior on error",
          "documentation": "Should the task halt when it encounters an error or continue to the next file.",
          "recommended_values": [
            "FAIL",
            "IGNORE"
          ]
        },
        {
          "name": "format.bytearray.separator",
          "type": "STRING",
          "importance": "MEDIUM",
          "default_value": "\n",
          "group": "Storage",
          "order_in_group": 4,
          "display_name": "Byte Array Line Separator",
          "documentation": "String inserted between records for ByteArrayFormat. Defaults to `\\n` and may contain escape sequences like `\\n`.  An input record that contains the line separator looks like multiple records in the storage object input."
        },
        {
          "name": "task.batch.size",
          "type": "INT",
          "importance": "HIGH",
          "default_value": "10",
          "group": "Storage",
          "order_in_group": 5,
          "documentation": "The number of files assigned to each task at a time",
          "display_name": "Task Batch Size"
        },
        {
          "name": "file.discovery.starting.timestamp",
          "type": "LONG",
          "importance": "HIGH",
          "default_value": "0",
          "group": "Storage",
          "order_in_group": 6,
          "display_name": "File Discovery Starting Timestamp",
          "documentation": "A unix timestamp (seconds since Jan 1, 1970 UTC) that denotes where to start processing files. Any file encountered with a creation time earlier than this will be ignored."
        },
        {
          "name": "gcs.poll.interval.ms",
          "type": "LONG",
          "importance": "MEDIUM",
          "default_value": "60000",
          "group": "Data polling policy",
          "order_in_group": 1,
          "display_name": "GCS poll interval (ms)",
          "documentation": "Frequency in milliseconds to poll for new or removed folders. This may result in updated task configurations starting to poll for data in added folders or stopping polling for data in removed folders"
        },
        {
          "name": "record.batch.max.size",
          "type": "INT",
          "importance": "MEDIUM",
          "default_value": "200",
          "group": "Data polling policy",
          "order_in_group": 2,
          "display_name": "Max records per poll",
          "documentation": "The maximum amount of records to return each time storage is polled."
        },
        {
          "name": "tasks.max",
          "type": "INT",
          "group": "Number of tasks for this connector",
          "required": true,
          "order_in_group": 1,
          "importance": "HIGH",
          "display_name": "Tasks max",
          "documentation": "The total number of tasks to run in parallel."
        }
      ],
      "connector_configs": [
        {
          "name": "consumer.override.security.protocol",
          "value": "SASL_SSL"
        },
        {
          "name": "consumer.override.sasl.mechanism",
          "value": "PLAIN"
        },
        {
          "name": "producer.override.security.protocol",
          "value": "SASL_SSL"
        },
        {
          "name": "producer.override.sasl.mechanism",
          "value": "PLAIN"
        },
        {
          "name": "admin.override.security.protocol",
          "value": "SASL_SSL"
        },
        {
          "name": "admin.override.sasl.mechanism",
          "value": "PLAIN"
        },
        {
          "name": "gcs.credentials.json"
        },
        {
          "name": "confluent.topic.bootstrap.servers",
          "value": "Placeholder value to pass connector validations"
        },
        {
          "name": "tasks.max"
        },
        {
          "name": "gcs.bucket.name"
        },
        {
          "name": "topic.regex.list"
        },
        {
          "name": "gcs.part.retries"
        },
        {
          "name": "gcs.retry.backoff.ms"
        },
        {
          "name": "schema.cache.size",
          "value": "1000"
        },
        {
          "name": "key.converter.schemas.enable",
          "value": "false"
        },
        {
          "name": "key.converter",
          "value": "org.apache.kafka.connect.storage.StringConverter"
        },
        {
          "name": "enhanced.avro.schema.support",
          "value": "true"
        },
        {
          "name": "storage.class",
          "value": "io.confluent.connect.gcs.storage.GcsStorage"
        },
        {
          "name": "behavior.on.null.values",
          "value": "ignore"
        },
        {
          "name": "topics.dir"
        },
        {
          "name": "mode",
          "value": "GENERIC"
        },
        {
          "name": "directory.delim"
        },
        {
          "name": "behavior.on.error",
          "switch": {
            "behavior.on.error": {
              "IGNORE": "ignore",
              "FAIL": "fail"
            }
          }
        },
        {
          "name": "format.bytearray.separator"
        },
        {
          "name": "format.bytearray.extension",
          "value": ".bin"
        },
        {
          "name": "task.batch.size"
        },
        {
          "name": "file.discovery.starting.timestamp"
        },
        {
          "name": "record.batch.max.size"
        },
        {
          "name": "gcs.poll.interval.ms"
        },
        {
          "name": "bucket.listing.max.objects.threshold",
          "value": "1000000"
        }
      ]
    },
    {
