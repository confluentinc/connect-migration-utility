      "connector_type": "SOURCE",
      "connector.class": "io.confluent.connect.rabbitmq.RabbitMQSourceConnector",
      "config_defs": [
        {
          "name": "rabbitmq.host",
          "type": "STRING",
          "required": true,
          "importance": "HIGH",
          "group": "How should we connect to RabbitMQ?",
          "order_in_group": 1,
          "display_name": "RabbitMQ host",
          "documentation": "RabbitMQ host to connect to."
        },
        {
          "name": "rabbitmq.username",
          "type": "STRING",
          "required": true,
          "importance": "HIGH",
          "group": "How should we connect to RabbitMQ?",
          "order_in_group": 2,
          "display_name": "RabbitMQ username",
          "documentation": "Username to authenticate with RabbitMQ."
        },
        {
          "name": "rabbitmq.password",
          "type": "PASSWORD",
          "required": true,
          "importance": "HIGH",
          "group": "How should we connect to RabbitMQ?",
          "order_in_group": 3,
          "display_name": "RabbitMQ password",
          "documentation": "Password to authenticate with RabbitMQ."
        },
        {
          "name": "rabbitmq.virtual.host",
          "type": "STRING",
          "importance": "HIGH",
          "group": "How should we connect to RabbitMQ?",
          "order_in_group": 4,
          "default_value": "/",
          "display_name": "RabbitMQ virtual host",
          "documentation": "The virtual host to use when connecting to the RabbitMQ broker."
        },
        {
          "name": "rabbitmq.port",
          "type": "INT",
          "importance": "HIGH",
          "group": "How should we connect to RabbitMQ?",
          "order_in_group": 5,
          "default_value": 5672,
          "display_name": "RabbitMQ port",
          "documentation": "RabbitMQ port to connect to. This should be set in accordance with chosen Security protocol."
        },
        {
          "name": "rabbitmq.queue",
          "type": "LIST",
          "required": true,
          "importance": "HIGH",
          "group": "RabbitMQ details",
          "order_in_group": 1,
          "display_name": "RabbitMQ queue(s)",
          "documentation": "RabbitMQ queue(s) to read from."
        },
        {
          "name": "batch.size",
          "type": "INT",
          "importance": "MEDIUM",
          "group": "RabbitMQ details",
          "order_in_group": 2,
          "display_name": "RabbitMQ batch size",
          "default_value": 1024,
          "documentation": "The maximum number of records to return to Connect for each poll, if there are more than this number of records already available from RabbitMQ."
        },
        {
          "name": "backoff.time.ms",
          "type": "INT",
          "importance": "MEDIUM",
          "group": "RabbitMQ details",
          "order_in_group": 3,
          "display_name": "RabbitMQ backoff time (in milliseconds)",
          "default_value": 100,
          "documentation": "The number of milliseconds to wait when no records are returned from the RabbitMQ queue."
        },
        {
          "name": "rabbitmq.security.protocol",
          "type": "STRING",
          "default_value": "PLAINTEXT",
          "importance": "HIGH",
          "group": "Security",
          "order_in_group": 1,
          "display_name": "Security protocol",
          "documentation": "The security protocol to use when connecting to RabbitMQ. Valid values are PLAINTEXT and SSL.",
          "recommended_values": [
            "PLAINTEXT",
            "SSL"
          ]
        },
        {
          "name": "rabbitmq.https.ssl.key.password",
          "type": "PASSWORD",
          "required": false,
          "importance": "HIGH",
          "group": "Security",
          "order_in_group": 2,
          "display_name": "SSL key password",
          "documentation": "The password of the private key in the key store file. This is optional for client."
        },
        {
          "name": "rabbitmq.https.ssl.keystorefile",
          "type": "PASSWORD",
          "required": false,
          "default_value": "",
          "importance": "HIGH",
          "group": "Security",
          "order_in_group": 3,
          "display_name": "Key Store",
          "documentation": "The key store containing server certificate. Only required if using SSL."
        },
        {
          "name": "rabbitmq.https.ssl.keystore.password",
          "type": "PASSWORD",
          "required": false,
          "importance": "HIGH",
          "group": "Security",
          "order_in_group": 4,
          "display_name": "Key Store password",
          "documentation": "The store password for the key store file. This is optional for a client and is only needed if key store is configured."
        },
        {
          "name": "rabbitmq.https.ssl.keystore.type",
          "type": "STRING",
          "required": false,
          "default_value": "JKS",
          "importance": "MEDIUM",
          "group": "Security",
          "order_in_group": 5,
          "display_name": "Key Store type",
          "documentation": "The file format of the key store file. This is optional for client."
        },
        {
          "name": "rabbitmq.https.ssl.truststorefile",
          "type": "PASSWORD",
          "required": false,
          "default_value": "",
          "importance": "HIGH",
          "group": "Security",
          "order_in_group": 6,
          "display_name": "Trust Store",
          "documentation": "The trust store containing server CA certificate. Only required if using SSL."
        },
        {
          "name": "rabbitmq.https.ssl.truststore.password",
          "type": "PASSWORD",
          "required": false,
          "importance": "HIGH",
          "group": "Security",
          "order_in_group": 7,
          "display_name": "Trust Store password",
          "documentation": "The password for the trust store file. If a password is not set, trust store file configured will still be used, but integrity checking is disabled. Trust store password is not supported for PEM format."
        },
        {
          "name": "rabbitmq.https.ssl.truststore.type",
          "type": "STRING",
          "required": false,
          "default_value": "JKS",
          "importance": "MEDIUM",
          "group": "Security",
          "order_in_group": 8,
          "display_name": "Trust Store type",
          "documentation": "The file format of the trust store file."
        }
      ],
